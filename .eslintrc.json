{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "warn",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-magic-numbers": ["warn", { "ignore": [8211] }],
    "quotes": ["error", "double", { "avoidEscape": true }]
  }
}
